

int foo(int i) {
    if(i == 0 || i == 1) {
        return 1;
    }
    else {
        return i * foo(i - 1);
    }
}

const int LIMIT = 10;

int main() {
    int a;
    while(a < LIMIT) {
        a = a + 1;
        if(a == 6) continue;
        if(a > 5) {
            break;
        }
    }

    int result = foo(a);

    return 0;
}
