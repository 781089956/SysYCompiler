
int main() {

    int a[2][2] = {1,2,3,4};
    int k = 0;
    int i = 0, j = 0;
    while(i < 2) {
        while(j < 2) {
            k = k + a[i][j];
            if(k >= 5) continue;
            j = j + 1;
        }
        i = i + 1;
        if(i == 1) {
            break;
        }
    }

    return 1;
}